load "../perturbedWalks.m2"

-- benchmarks gb, gfanBuchberger, and groebnerWalk on toric ideals
-- over Q, generated by random 4x7 matrices with integer entries between 0 and 16


R = QQ[a..h]
A = matrix{ for i to 7 list random(0,16), for i to 7 list random(0,16), for i to 7 list random(0,16), for i to 7 list random(0,16)} 

I = toBinomial(toricMarkov(A), R)

R2 = QQ[a..h, MonomialOrder => Lex]

G1 = gb I

I2 = sub(I,R2)

 

t1 = elapsedTiming gb I2

t2 = elapsedTiming gfanBuchberger(I2)

--setWalkTrace 3 

t3 = elapsedTiming groebnerWalk(G1, R2) 

t4 = elapsedTiming groebnerWalk(G1, R2, Strategy => Generic )


